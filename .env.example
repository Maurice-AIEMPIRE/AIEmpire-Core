# AI Empire Core Environment Configuration
# Copy to .env and fill in actual values

# Moonshot Kimi API
MOONSHOT_API_KEY=sk-your-api-key-here

# GitHub API
GITHUB_TOKEN=ghp_your-token-here
GITHUB_REPO=mauricepfeifer-ctrl/AIEmpire-Core
GITHUB_WORKSPACE=/path/to/workspace

# OpenClaw Paths
OPENCLAW_HOME=~/.openclaw
BRAIN_SYSTEM_DB=~/.openclaw/brain-system/synapses.db

# Services
OLLAMA_HOST=http://localhost:11434
REDIS_HOST=localhost
REDIS_PORT=6379
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# n8n
N8N_HOST=http://localhost:5678

# X/Twitter (if using)
TWITTER_API_KEY=
TWITTER_API_SECRET=

# Gumroad (if using)
GUMROAD_API_KEY=

# Debug
DEBUG=false
LOG_LEVEL=info
