{
  "providers": {
    "moonshot": {
      "type": "openai_compat",
      "base_url": "https://api.moonshot.ai/v1",
      "api_key_env": "MOONSHOT_API_KEY"
    },
    "openai": {
      "type": "openai_compat",
      "base_url": "https://api.openai.com/v1",
      "api_key_env": "OPENAI_API_KEY"
    },
    "openrouter": {
      "type": "openai_compat",
      "base_url": "https://openrouter.ai/api/v1",
      "api_key_env": "OPENROUTER_API_KEY",
      "headers_env": {
        "HTTP-Referer": "OPENROUTER_REFERRER",
        "X-Title": "OPENROUTER_TITLE"
      }
    }
  },
  "models": {
    "cheap": [
      {
        "provider": "moonshot",
        "model": "moonshot-v1-8k",
        "max_output_tokens": 1400,
        "temperature": 0.5
      },
      {
        "provider": "openai",
        "model": "gpt-4o-mini",
        "max_output_tokens": 1400,
        "temperature": 0.5
      },
      {
        "provider": "openrouter",
        "model": "openai/gpt-4o-mini",
        "max_output_tokens": 1400,
        "temperature": 0.5
      }
    ],
    "balanced": [
      {
        "provider": "moonshot",
        "model": "kimi-k2.5",
        "max_output_tokens": 2000,
        "temperature": 0.6
      },
      {
        "provider": "openai",
        "model": "gpt-4o",
        "max_output_tokens": 2000,
        "temperature": 0.6
      },
      {
        "provider": "openrouter",
        "model": "openai/gpt-4o",
        "max_output_tokens": 2000,
        "temperature": 0.6
      }
    ],
    "premium": [
      {
        "provider": "moonshot",
        "model": "kimi-k2.5",
        "max_output_tokens": 2400,
        "temperature": 0.6
      },
      {
        "provider": "openai",
        "model": "gpt-4o",
        "max_output_tokens": 2400,
        "temperature": 0.6
      },
      {
        "provider": "openrouter",
        "model": "openai/gpt-4o",
        "max_output_tokens": 2400,
        "temperature": 0.6
      }
    ]
  },
  "routing": {
    "orchestrator": "balanced",
    "ideation": "cheap",
    "nuggets_ideation": "cheap",
    "writer": "cheap",
    "writer_threads": "cheap",
    "writer_tweets": "cheap",
    "writer_premium": "cheap",
    "refiner": "cheap",
    "strategy": "balanced",
    "nugget_extraction": "cheap"
  },
  "fallback_order": [
    "cheap",
    "balanced"
  ]
