{
  "version": 2,
  "description": "Ant Protocol + OpenClaw - Ollama-First Mode (zero cloud cost)",

  "agent_to_task_mapping": {
    "research": {
      "task_type": "research",
      "default_model": "qwen-7b",
      "skills": ["research", "analysis", "trends"],
      "priority": 5
    },
    "content": {
      "task_type": "content",
      "default_model": "qwen-7b",
      "skills": ["content", "writing", "creative"],
      "priority": 5
    },
    "product": {
      "task_type": "revenue",
      "default_model": "qwen-7b",
      "skills": ["revenue", "marketing", "sales"],
      "priority": 6
    },
    "finance": {
      "task_type": "revenue",
      "default_model": "qwen-14b",
      "skills": ["revenue", "finance", "analytics"],
      "priority": 7
    },
    "community": {
      "task_type": "content",
      "default_model": "qwen-7b",
      "skills": ["content", "community", "engagement"],
      "priority": 4
    },
    "ops": {
      "task_type": "general",
      "default_model": "qwen-7b",
      "skills": ["general", "planning", "ops"],
      "priority": 5
    },
    "analytics": {
      "task_type": "research",
      "default_model": "deepseek-r1",
      "skills": ["research", "analytics", "data"],
      "priority": 5
    },
    "code": {
      "task_type": "code",
      "default_model": "qwen-14b",
      "skills": ["code", "devops", "testing"],
      "priority": 6
    }
  },

  "colony": {
    "id": "empire-alpha",
    "max_workers": 50,
    "worker_pools": [
      {
        "name": "general",
        "count": 3,
        "skills": ["general", "content", "research"],
        "model": "qwen-7b"
      },
      {
        "name": "code",
        "count": 2,
        "skills": ["code", "devops", "testing"],
        "model": "qwen-14b"
      },
      {
        "name": "revenue",
        "count": 2,
        "skills": ["revenue", "leads", "sales", "marketing"],
        "model": "qwen-7b"
      }
    ]
  },

  "model_routing": {
    "primary": "ollama",
    "fallback_order": ["ollama", "gemini", "kimi"],
    "ollama_models": {
      "fast": "qwen-7b",
      "smart": "qwen-14b",
      "reasoning": "deepseek-r1"
    },
    "cloud_models": {
      "gemini_flash": "gemini-cloud-flash",
      "gemini_pro": "gemini-cloud-pro",
      "kimi": "kimi"
    }
  },

  "pheromone_defaults": {
    "revenue": 8.0,
    "code": 5.0,
    "content": 5.0,
    "research": 3.0,
    "general": 1.0
  },

  "endpoints": {
    "ant_protocol_api": "http://localhost:8900",
    "skybot_api": "http://localhost:8901",
    "litellm_proxy": "http://localhost:4000"
  }
}
