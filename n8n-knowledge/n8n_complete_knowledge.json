  "code_examples": {
    "docker_compose_production": "version: '3.8'\nservices:\n  n8n:\n    image: n8nio/n8n:latest\n    restart: always\n    ports:\n      - '5678:5678'\n    environment:\n      - N8N_HOST=0.0.0.0\n      - N8N_PORT=5678\n      - N8N_PROTOCOL=https\n      - N8N_ENCRYPTION_KEY=${N8N_ENCRYPTION_KEY}\n      - DB_TYPE=postgresdb\n      - DB_POSTGRESDB_HOST=postgres\n      - DB_POSTGRESDB_PORT=5432\n      - DB_POSTGRESDB_DATABASE=n8n\n      - DB_POSTGRESDB_USER=n8n\n      - DB_POSTGRESDB_PASSWORD=${DB_PASSWORD}\n      - GENERIC_TIMEZONE=Europe/Berlin\n      - EXECUTIONS_MODE=queue\n      - QUEUE_BULL_REDIS_HOST=redis\n    volumes:\n      - n8n_data:/home/node/.n8n\n    depends_on:\n      - postgres\n      - redis\n  postgres:\n    image: postgres:16\n    restart: always\n    environment:\n      - POSTGRES_DB=n8n\n      - POSTGRES_USER=n8n\n      - POSTGRES_PASSWORD=${DB_PASSWORD}\n    volumes:\n      - postgres_data:/var/lib/postgresql/data\n  redis:\n    image: redis:7-alpine\n    restart: always\n    volumes:\n      - redis_data:/data\nvolumes:\n  n8n_data:\n  postgres_data:\n  redis_data:",
    "api_create_workflow": "curl -X POST http://localhost:5678/api/v1/workflows \\\n  -H 'X-N8N-API-KEY: your-api-key' \\\n  -H 'Content-Type: application/json' \\\n  -d '{\"name\": \"My Workflow\", \"nodes\": [], \"connections\": {}}'",
    "api_execute_workflow": "curl -X POST http://localhost:5678/api/v1/workflows/{id}/execute \\\n  -H 'X-N8N-API-KEY: your-api-key' \\\n  -H 'Content-Type: application/json' \\\n  -d '{\"data\": {\"key\": \"value\"}}'",
    "python_n8n_api_client": "import aiohttp\n\nclass N8NClient:\n    def __init__(self, base_url='http://localhost:5678', api_key=''):\n        self.base_url = base_url\n        self.headers = {'X-N8N-API-KEY': api_key, 'Content-Type': 'application/json'}\n\n    async def list_workflows(self):\n        async with aiohttp.ClientSession() as s:\n            async with s.get(f'{self.base_url}/api/v1/workflows', headers=self.headers) as r:\n                return await r.json()\n\n    async def execute_workflow(self, workflow_id, data=None):\n        async with aiohttp.ClientSession() as s:\n            async with s.post(f'{self.base_url}/api/v1/workflows/{workflow_id}/execute',\n                              headers=self.headers, json={'data': data or {}}) as r:\n                return await r.json()"
  },

